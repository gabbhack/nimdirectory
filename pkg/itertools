<div class="line"><div class="line-content"></div></div><div class="line"><div class="line-content"><span class="kwd">for</span> <span class="ide">key</span>, <span class="ide">group</span> <span class="kwd">in</span> <span class="ide">numbers</span><span class="op">.</span><span class="ide">groupBy</span>(<span class="ide">x</span> <span class="op">=&gt;</span> <span class="ide">x</span> <span class="kwd">mod</span> <span class="num">2</span> <span class="op">==</span> <span class="num">0</span>):</div></div><div class="line"><div class="line-content">  <span class="ide">echo</span> <span class="str">&quot;key: &quot;</span>, <span class="ide">key</span>, <span class="str">&quot; group: &quot;</span>, <span class="ide">group</span></div></div><div class="line"><div class="line-content"><span class="com"># key: true, group: @[8, 4, 2, 6]; key: false, group: @[1, 3, 7, 5, 9]</span></div></div><div class="line"><div class="line-content"></div></div><div class="line"><div class="line-content"><span class="kwd">for</span> <span class="ide">key</span>, <span class="ide">group</span> <span class="kwd">in</span> <span class="ide">word</span><span class="op">.</span><span class="ide">groupBy</span>():</div></div><div class="line"><div class="line-content">  <span class="ide">echo</span> <span class="ide">group</span></div></div><div class="line"><div class="line-content"><span class="com"># @[&apos;a&apos;, &apos;a&apos;, &apos;a&apos;, &apos;a&apos;, &apos;a&apos;]; @[&apos;b&apos;, &apos;b&apos;]; @[&apos;c&apos;]; @[&apos;d&apos;]; @[&apos;r&apos;, &apos;r&apos;]</span></div></div><div class="line"><div class="line-content"></div></div><div class="line"><div class="line-content"><span class="kwd">for</span> <span class="ide">key</span>, <span class="ide">group</span> <span class="kwd">in</span> <span class="ide">groupConsecutive</span>(<span class="str">&quot;aaabaabb&quot;</span>):</div></div><div class="line"><div class="line-content">  <span class="ide">echo</span> <s